<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SecondActivity">

  <ImageView
      android:id="@+id/WInner_ID"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/ic_launcher__" />

  <TextView
      android:id="@+id/Winner_Logo"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="157dp"
      android:layout_marginTop="530dp"
      android:layout_marginEnd="159dp"
      android:text="Winner"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="@+id/WInner_ID"
      app:layout_constraintEnd_toEndOf="@+id/WInner_ID"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="@+id/WInner_ID"
      app:layout_constraintTop_toTopOf="@+id/WInner_ID"
      app:layout_constraintVertical_bias="0.0" />

  <TextView
      android:id="@+id/Winner_ID"
      android:layout_width="114dp"
      android:layout_height="21dp"
      android:text="TextView"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>
